{
    "name": "storykube/php-summariser",
    "version": "1.0.0",
    "description": "Library to generate extractive summary with PHP.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ottavio Fogliata",
            "email": "ottavio.fogliata@storykube.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "7.*",
        "nlp-tools/nlp-tools": "1.0.*@dev",
        "ext-ctype": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "StorykubeLibrary\\Summariser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StorykubeLibrary\\Summariser\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "php ./vendor/bin/phpunit --colors='always' $(pwd)/tests"
    }
}
